{
  "AllowedHosts": "*",
  "Serilog": {
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\log.txt",
          "rollingInterval": "Day",
          "fileSizeLimitBytes": 5000,
          "rollOnFileSizeLimit": true,
          "retainedFileCountLimit": null
        }
      }
    ]
  }
}
